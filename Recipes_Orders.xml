	<RecipeDef>
		<defName>RDOrder_FineMeal</defName>
		<label>order a fine meal</label>
		<description>Order some food from RimDash.</description>
		<jobString>Ordering at payphone.</jobString>
		<workAmount>2000</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>2.0</count>
			</li>
		</ingredients>
		<products>
			<MealFine>1</MealFine>
		</products>
		<recipeUsers>
			<li>RD_Payphone</li>
		</recipeUsers>
	</RecipeDef>

